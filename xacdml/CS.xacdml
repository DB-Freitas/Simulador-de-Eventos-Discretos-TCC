    <act id="FB_6">
        <stat type="CONST" parm1="0.01"/>
        <entity_class prev="Q_4" next="Q_5"/>
        <entity_class prev="N_TRAFFIC_SENSOR_1" next="N_TRAFFIC_SENSOR_1"/>
    </act>

    <act id="FB_7">
        <stat type="CONST" parm1="0.02"/>
        <entity_class prev="Q_5" next="Q_6"/>
        <entity_class prev="N_TRAFFIC_SENSOR_2" next="N_TRAFFIC_SENSOR_2"/>
    </act>

    <act id="FB_2">
        <stat type="CONST" parm1="0.3"/>
        <entity_class prev="Q_12" next="Q_7"/>
        <entity_class prev="TRAFFIC_LIGHTS" next="TRAFFIC_LIGHTS"/>
    </act>

    <act id="FB_1">
        <stat type="CONST" parm1="0.5"/>
        <entity_class prev="Q_6" next="OUT"/>
        <entity_class prev="Q_7" next="OUT"/>
        <entity_class prev="SCCI_MODULE" next="SCCI_MODULE"/>
    </act>


    <!-- SaÃ­da -->
    <destroy id="DESTROY_OUT" class="INTERSECTION">
        <prev dead="OUT"/>
    </destroy>

</acd>
